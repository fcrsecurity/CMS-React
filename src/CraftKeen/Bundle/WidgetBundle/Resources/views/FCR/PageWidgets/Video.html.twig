{% set fields = ['image','videoUrl','icon'] %}

{% set widget = widget|merge({'data' : checkWidgetFields(widget.data, fields) }) %}

{{
    react_component(
        widget.dataType, {
            'props':{
                "isLoggedIn": true,
                "id": widget.id,
                "image" : widget.data.image,
                "videoUrl" : widget.data.videoUrl,
                "icon" : widget.data.icon,
                "config": widget.config
            }
        }
    )
}}
