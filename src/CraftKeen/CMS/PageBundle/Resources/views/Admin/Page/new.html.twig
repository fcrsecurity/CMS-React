{% extends 'CraftKeenCMSAdminBundle::layout.html.twig' %}

{% block admin_block_title %}Add Page{% endblock %}

{% block admin_block_breadcrumb %}
    <li><a href="{{ path('craftkeen_cms_page_admin_page_index') }}">Pages</a></li>
    <li class="active">Add Page</li>
{% endblock %}

{% block admin_block_body %}

    {{ form_start(form) }}
        {{ form_widget(form) }}
        <input type="submit" class="btn btn-success" value="Create" />
    {{ form_end(form) }}

    <hr />

    <ul class="list-inline list-unstyled">
        <li>
            <a href="{{ path('craftkeen_cms_page_admin_page_index') }}" class="btn btn-default">Back to the list</a>
        </li>
    </ul>
{% endblock %}