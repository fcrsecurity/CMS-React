{% set latest = financialReports|last %}
{% if financialReports is not empty and latest is not empty%}
	<div class="buffer-6-padding-top buffer-6-padding-bottom FinancialReports inline-mode">
	<a href="{{ path('admin_investors_financial-report_index') }}" class="button-edit-element" title="Edit element"></a>
		<div class="row ">
			<div class="col-md-6 col-lg-6 left-part">
				<div class="row">
					<div class="col-lg-12 text-center head-part">
						<h2>{{ latest.year }}</h2>
					</div>
				</div>
				<div class="row">

					<div class="col-md-4 col-md-offset-2  text-center">
						{% if latest.Q1 is defined and latest.Q1|length > 0 %}
						<div class="thumbnails">
							<a href="{{ latest.Q1 }}" target="_blank">
