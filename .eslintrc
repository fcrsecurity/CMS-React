{
	"parser": "babel-eslint",
	"extends": [
		"eslint:recommended",
		"plugin:react/recommended"
	],
	"env": {
	  "browser": true,
	  "jest": true,
	  "es6": true,
	  "node": true
	},
	"parserOptions": {
		"ecmaVersion": 6,
		"sourceType": "module",
		"ecmaFeatures": {
			"jsx": true
		}
	},
	"rules": {
		"semi": ["error", "always"],
		"indent": ["error", "tab", {
			"SwitchCase": 1
		}],
		"no-console": 0
	}
}
